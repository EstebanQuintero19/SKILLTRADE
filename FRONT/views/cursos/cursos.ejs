    <main class="container mt-5 pt-5">
        <!-- Cursos Destacados -->
        <section id="destacados" class="mb-5">
            <h2 class="h3 fw-bold mb-4">Cursos destacados</h2>
            <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-4">
                <% 
                const cursosArray = Array.isArray(cursos) ? cursos : (cursos?.data ? cursos.data : []);
                const cursosDestacados = cursosArray && cursosArray.length > 0 ? 
                    cursosArray.filter(curso => curso.destacado || curso.estadisticas?.calificacionPromedio >= 4.5).slice(0, 3) : 
                    cursosArray.slice(0, 3);
                %>
                
                <% if (cursosDestacados.length > 0) { %>
                    <% cursosDestacados.forEach(curso => { %>
                        <div class="col-md-4">
                            <div class="card p-3 d-flex flex-column h-100 card-course">
                                <img src="<%= curso.imagen || '/images/placeholder-course.jpg' %>" alt="<%= curso.titulo %>">
                                <div class="card-text-wrapper">
                                    <div class="card-title-tittle">
                                        <h4 class="fw-semibold">
                                            <a href="/cursos/<%= curso._id %>" style="color: #111; text-decoration: none;">
                                                <%= curso.titulo.toUpperCase() %>
                                            </a>
                                        </h4>
                                    </div>
                                    <div class="course-rating mb-2">
                                        <% const rating = curso.estadisticas?.calificacionPromedio || 0; %>
                                        <% for (let i = 1; i <= 5; i++) { %>
                                            <% if (i <= Math.floor(rating)) { %>
                                                <i class="bi bi-star-fill text-warning"></i>
                                            <% } else if (i - 0.5 <= rating) { %>
                                                <i class="bi bi-star-half text-warning"></i>
                                            <% } else { %>
                                                <i class="bi bi-star text-warning"></i>
                                            <% } %>
                                        <% } %>
                                        <span class="ms-2 text-muted">(<%= rating.toFixed(1) %>)</span>
                                    </div>
                                    <div class="card-subtitle-limit">
                                        <h6 class="fw-semibold">
                                            <%= curso.descripcion || 'Descripción no disponible' %>
                                        </h6>
                                    </div>
                                </div>
                                <div class="mt-auto btn-action">
                                    <button type="button" class="btn btn-primary btn-price">
                                        COP <%= (curso.precio || 0).toLocaleString('es-CO') %>
                                    </button>
                                    <button type="button" class="btn btn-primary btn-cart" 
                                            data-course-id="<%= curso._id %>"
                                            data-bs-toggle="modal" data-bs-target="#cartModal">
                                        <i class="bi bi-cart"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    <% }) %>
                <% } else { %>
                    <div class="col-12 text-center py-5">
                        <p class="text-muted">No hay cursos destacados disponibles en este momento.</p>
                    </div>
                <% } %>
            </div>
        </section>

        <section class="container py-5 importan-courses" id="valorados">
            <div class="row mb-4">
